<Window x:Class="FifthLab.CashierWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FifthLab"
        mc:Ignorable="d"
        Title="CashierWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Button Content="Customers" FontSize="15" Click="Button_Click"/>
            <Button Content="Tickets" Grid.Column="1" FontSize="15" Click="Button_Click_1"/>
            <Button Content="Reserves" Grid.Column="2" FontSize="15" Click="Button_Click_2"/>
            <Button Content="Sales" Grid.Column="3" FontSize="15" Click="Button_Click_3"/>
        </Grid>
        <Frame x:Name="Pages" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
